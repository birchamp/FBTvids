{
  "expo": {
    "name": "Foundations BT",
    "slug": "fbt-videos-app",
    "version": "1.0.0",
    "platforms": [
      "android"
    ],
    "android": {
      "package": "com.unfoldingword.fbtapp",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#2c3e50"
      }
    },
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c3e50"
    },
    "assetBundlePatterns": [
      "assets/**/*",
      "thumbs/**/*",
      "i18n/**/*"
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d292c857-f3d4-4c3c-88bf-4bbc99edee77"
      }
    },
    "plugins": [
      "./plugins/withFbtAssetPack",
      "expo-asset"
    ]
  }
}
